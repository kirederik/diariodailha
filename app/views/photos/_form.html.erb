<%= simple_form_for(@photo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :album_id, :as => :hidden ,:value => @photo.album_id %>
    <%= f.file_field :pic, :required => true %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
