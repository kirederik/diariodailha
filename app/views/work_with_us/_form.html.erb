<%= simple_form_for(@work_with_u) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.file_field :curriculum %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
